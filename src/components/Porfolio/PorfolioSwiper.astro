---
import "swiper/css";
import "swiper/css/navigation";
import 'swiper/css/effect-coverflow';
const { id, images } = Astro.props;
---

<div class=`mySwiper${id} swiper w-full h-full rounded-md relative`>
    <div class="swiper-wrapper">
        { images.map((image:any) => (
            <div class="swiper-slide rounded-md"><img src=`${image.src}` alt=`${image.alt}` /></div>
        ))}
    </div>
</div>
      
